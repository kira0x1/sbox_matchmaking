{
  "name": "sbox_matchmaking",
  "version": "1.0.0",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "start": "bun run .",
    "dev": "bun run --hot src/index.ts",
    "watch": "bun run --watch src/index.ts",
    "ts:check": "tsc --noemit",
    "ls": "bun pm ls",
    "clean": "rimraf ./out",
    "db": "bunx drizzle-kit studio",
    "seed": "bun run src/database/seed.ts",
    "generate": "bun run drizzle-kit generate",
    "migrate": "bun run drizzle-kit migrate",
    "push": "bun run drizzle-kit push",
    "drop": "bun run drizzle-kit drop",
    "updatedb": "bun run generate && bun run migrate && bun run push"
  },
  "author": "kira",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.8",
    "cli-color": "^2.0.4",
    "drizzle-orm": "^0.30.10",
    "hono": "^4.3.8",
    "jsonwebtoken": "^9.0.2",
    "ky": "^1.2.4",
    "pg": "^8.11.5"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@types/bun": "latest",
    "@types/cli-color": "^2.0.6",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/pg": "^8.11.6",
    "drizzle-kit": "^0.21.2",
    "rimraf": "^5.0.7"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}